(set-logic QF_S)
(set-option :parse-transducers true)
(set-option :produce-models true)
(define-fun-rec toLower ((x String) (y String)) Bool
   (or (and (= x "") (= y ""))
       (and (not (= x "")) (not (= y ""))
           (= (char.code (str.head y))
               (ite (and (<= 65 (char.code (str.head x)))
                       (<= (char.code (str.head x)) 90))
                   (+ (char.code (str.head x)) 32)
                   (char.code (str.head x))))
           (toLower (str.tail x) (str.tail y))))
)
(declare-fun r1[$i2] () String)
(declare-fun $r3 () String)
(declare-fun L12  () String)
(declare-fun L7  () String)
(declare-fun L5  () String)
(declare-fun L11  () String)
(declare-fun L1  () String)
(declare-fun L0  () String)
(assert (toLower $r3  L0 ))
(assert (toLower "description"  L1 ))
(assert (toLower "table"  L11 ))
(assert (toLower "database"  L5 ))
(assert (toLower "name"  L7 ))
(assert (toLower r1[$i2]  L12 ))

(assert (not (= L0 L1 )))
(assert (= L0 L1 ))
(assert (= L0 L5 ))
(assert (= L0 L7 ))
(assert (not (= L0 L7 )))
(assert (not (= L0 L11 )))
(assert (= L12 "html" ))
(assert (not (= L12 "sql" )))
(assert (not (= L0 L7 )))
(check-sat)
(get-model)
(exit)
